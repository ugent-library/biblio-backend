<div class="form-group row">
    <label class="col-sm-3 col-form-label align-self-start">
        {{.Label}}{{if .Required}}&nbsp;<abbr class="required" title="Required">*</abbr>{{end}}
    </label>
    <div class="col-sm-9">
        {{if .Values}}
        <ul class="list-unstyled">
        {{range .Values}}
            <li>{{.}}</li>
        {{end}}
        </ul>
        {{else}}
        <span>-</span>
        {{end}}
    </div>
</div>