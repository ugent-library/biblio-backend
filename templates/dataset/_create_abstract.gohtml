{{define "create-abstract"}}
    <div hx-swap-oob="innerHTML:#abstract-modals"></div>
    <div hx-swap-oob="innerHTML:#abstracts-body">
        {{template "abstracts-table" .}}
    </div>
{{end}}

{{define "create-abstract-failed"}}
    <div hx-swap-oob="innerHTML:#abstract-modals">
        {{template "add-abstract" .}}
    </div>
{{end}}
