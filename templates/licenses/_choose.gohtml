{{if .D.Suggestions}}
<ul class="list-group">
    {{range .D.Suggestions}}
    <li class="list-group-item list-group-item-action d-flex justify-content-between align-items-center autocomplete-hit" data-value="{{.ID}}">
        {{.Description}}
        <span class="badge badge-pill badge-default">{{.ID}}</span>
    </li>
    {{end}}
</ul>
{{end}}